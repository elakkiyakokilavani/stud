#include<stdio.h>
int main()
{
int a,b,c;
printf("enter the three numbers");
scanf("%d %d %d",&a,&b,&c);
if(a>b&&a>c)
{
printf("the largest number=%d",a);
}
else if(b>a&&b>c)
{
printf("the largest number=%d",b);
}
else if(c>a&&c>b)
{
printf("the largest number=%d",c);
}
else
{
printf("all are equal number or any two values are equal");
}
return(0);
}
